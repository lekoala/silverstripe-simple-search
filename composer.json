{
    "name": "lekoala/silverstripe-simple-search",
    "description": "Simple website search in SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "search",
        "module",
        "cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "LeKoala",
            "email": "thomas@lekoala.be"
        }
    ],
    "require": {
        "php": ">=7.2",
        "silverstripe/cms": "^4"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "silverstripe/googlesitemaps": "^2.1"
    },
    "extra": {
        "installer-name": "simple-search"
    },
    "autoload": {
        "psr-4": {
            "LeKoala\\SimpleSearch\\": "src/",
            "LeKoala\\SimpleSearch\\Test\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-simple-search/issues"
    },
    "scripts": {
        "lint": "phpcs src/ tests/",
        "lint-clean": "phpcbf src/ tests/",
        "test": "phpunit -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
